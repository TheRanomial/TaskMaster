{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma"],
        "buildCommand": "npm run build && prisma generate"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
